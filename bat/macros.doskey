ip=ipconfig $*
ls=dir /B $1
pmax=liquidctl --match kraken set pump speed 100
pmin=liquidctl --match kraken set pump speed 40
pump=liquidctl --match kraken set pump speed $*
loff=liquidctl --match kraken set sync color off
lon=liquidctl --match kraken set sync color fixed ff0000 && start "" "C:\Users\%USERNAME%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\WhirlwindFX\SignalRgb.lnk" && timeout /t 3 && taskkill /im SignalRgb.exe
